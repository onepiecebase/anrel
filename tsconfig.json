{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "declaration": true,
    "resolveJsonModule": true,
    "paths": {
      "@anrel/*": ["@anrel-command/*", "@anrel-core/*", "@anrel-script/*"]
    }
  },
  "typeRoots": ["node_modules/@types"],
  "exclude": ["node_modules"]
}
