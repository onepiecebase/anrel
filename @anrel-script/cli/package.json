{
  "name": "@anrel/cli",
  "version": "0.0.1-alpha.1",
  "license": "MIT",
  "bin": "./bin/cli",
  "scripts": {
    "prepublishOnly": "npm run product",
    "lint-stage:format": "prettier --config ./prettierrc.js --write --",
    "lint-stage:lint:ts": "eslint --ext .ts --fix --",
    "format": "prettier --config ./prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,md,json,yml,yaml}\"",
    "lint:ts": "eslint --ext .ts ./src --fix",
    "lint": "npm run lint:ts",
    "unitest": "ts-mocha --paths -p ./test/tsconfig.json test/**/*.spec.ts",
    "unitest:watch": "npm run unitest -- --watch --watch-files \"src/**/*.ts,test/**/*.spec.ts\" -R min",
    "test": "npm run unitest",
    "develop": "rimraf ./libs && tsc -p ./src --outDir ./libs --declaration --watch",
    "product": "rimraf ./libs && tsc -p ./src --outDir ./libs --declaration",
    "start": "npm run develop"
  },
  "dependencies": {
    "@anrel/bootstrap": "^0.0.1-alpha.1",
    "@anrel/version": "^0.0.1-alpha.1",
    "@anrel/publish": "^0.0.1-alpha.1",
    "commander": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "eslint": "^6.8.0",
    "@types/npmlog": "^4.1.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.8.1"
  }
}
